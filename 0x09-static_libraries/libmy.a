/* 0-isupper.c */
int _isupper(int c)
{
    return (c >= 'A' && c <= 'Z');
}

/* 0-memset.c */
char *_memset(char *s, char b, unsigned int n)
{
    unsigned int i;

    for (i = 0; i < n; i++)
        s[i] = b;

    return (s);
}

